extends layout

block content
  div.row
    div.col-md-6
      h3 QASHETH
      table.display#qe
        thead
          tr
            th Exchange
            th Ask
            th Bid
            th 
              span.label.quoine quoine
            th 
              span.label.qryptos qryptos
            th 
              span.label.bitfinex bitfinex
              
    div.col-md-6
      h3 QASHBTC
      table.display#qb
        thead
          tr
            th Action
            th Ask
            th Bid
            th 
              span.label.quoine quoine
            th 
              span.label.qryptos qryptos
            th 
              span.label.bitfinex bitfinex
    
    div.col-md-6
      h3 QASH USD SGD
      table.display#qus
        thead
          tr
            th Exchange
            th Ask
            th Bid
            th Percentage
            th SGDUSD
            th Spread

block script
  script(src='/javascripts/qe.js')
  script(src='/javascripts/qb.js')
  script(src='/javascripts/qus.js')
