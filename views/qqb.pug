extends layout

block content
  div.row
    div.col-md-6
      h3 QASHETH
      table.display#qe
        thead
          tr
            th Exchange
            th Ask
            th Bid
            th 
              span.label.quoine quoine
            th 
              span.label.qryptos qryptos
            th 
              span.label.bitfinex bitfinex
              
    div.col-md-6
      h3 QASHBTC
      table.display#qb
        thead
          tr
            th Exchange
            th Ask
            th Bid
            th 
              span.label.quoine quoine
            th 
              span.label.qryptos qryptos
            th 
              span.label.bitfinex bitfinex
    
    div.col-md-6
      h3 QASH USD SGD
      table.display#qus
        thead
          tr
            th Exchange
            th Ask
            th Bid
            th Percentage
            th SGDUSD
            th Spread

    div.col-md-12
      h3 ETHBTC
      table.display#eb
        thead
          tr
            th Exchange
            th Ask
            th Bid
            th 
              span.label.quoine quoine
            th 
              span.label.qryptos qryptos
            th 
              span.label.bitfinex bitfinex
            th 
              span.label.poloniex poloniex
            th 
              span.label.bittrex bittrex

    div.col-md-6
      h3 ETHUSD
      table.display#eu
        thead
          tr
            th Exchange
            th Ask
            th Bid
            th 
              span.label.quoine quoine
            th 
              span.label.bitfinex bitfinex
            th 
              span.label.poloniex poloniex
            th 
              span.label.bittrex bittrex

block script
  script(src='/javascripts/qe.js')
  script(src='/javascripts/qb.js')
  script(src='/javascripts/qus.js')
  script(src='/javascripts/eb.js')
  script(src='/javascripts/eu.js')