extends layout

block content
  div(class='page-header')
    h1 QASH ETH
  table(class='display', id='qe')
    thead
      tr
        th Exchange
        th Ask
        th Bid
        th Percentage
        th Amount
        th Spread
        
  div(class='page-header')
    h1 QASH BTC
  table(class='display', id='qb')
    thead
      tr
        th Action
        th Ask
        th Bid
        th Percentage
        th Amount
        th Spread
  
  div(class='page-header')
    h1 Bitfinex QASH to BTC
  table(class='display', id='qtb')
    thead
      tr
        th Currency
        th ETH
        th BTC
        th Percentage
  
  div(class='page-header')
    h1 Quoinex BTC to ETH
  table(class='display', id='bte')
    thead
      tr
        th Currency
        th USD
        th SGD
        th ETH
        th Percentage

block script
  script(src='/javascripts/qe.js')
  script(src='/javascripts/qb.js')
  script(src='/javascripts/qtb.js')
  script(src='/javascripts/bte.js')
